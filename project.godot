config_version=5

[application]

config/name="template"
run/main_scene="uid://ciqb3vy1q1je"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

debug_ghost_autoload="*res://addons/tools_mincuz/scripts/debug_ghost_autoload.gd"
